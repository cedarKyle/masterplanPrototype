#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@shapediver/sdk.sdtf-v1/bin/sdtf-v1.js" "$@"
else 
  exec node  "$basedir/../@shapediver/sdk.sdtf-v1/bin/sdtf-v1.js" "$@"
fi
